    <!--This is the Data dictionary for The flyouts for PreviouslypassedYear3 on the Selecting a major page.
    The idea of using a flyout was to hid all the information on the page but still being able to access it if you wanted.-->
    <Flyout x:Key="SecurityMajor2" x:Name="SecurityMajor2">
        <!--Prequistes and Major electives Text boxes have a white background so they are distinctive
            from the Major Colour Background eg. red For newtorking Majors.All text boxes are read only so the user can not change them-->
        <StackPanel Background="Red">
            <TextBox
                Background="White"
                IsReadOnly="true"
                Text="Prequsites" />
            <TextBox IsReadOnly="true" Text="T201 - Networking Funtalmentals" />
            <TextBox IsReadOnly="true" Text="T206 - CISCO RSE (Networks)" />
            <TextBox IsReadOnly="true" Text="T211 - Systems Security" />
            <TextBox
                Background="White"
                IsReadOnly="true"
                Text="Major Electives" />
            <TextBox IsReadOnly="true" Text="T311 - Systems Administration" />
            <TextBox IsReadOnly="true" Text="T312 - Network Security CCNA Security" />
        </StackPanel>
    </Flyout>
