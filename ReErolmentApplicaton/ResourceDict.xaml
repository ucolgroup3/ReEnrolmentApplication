<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ReErolmentApplicaton">
     <!--This is the Data dictionary for The flyouts for PreviouslypassedYear2 on the Selecting a major page.
    The idea of using a flyout was to hid all the information on the page but still being able to access it if you wanted.-->
    <Flyout x:Key="SecurityMajor" x:Name="SecurityMajor" >
            <!-- It uses a stack panel to order them and stay symetrical-->
        <StackPanel Background="Red" BorderBrush="Black">
            <!--Prequistes and Major electives Text boxes have a white background so they are distinctive
            from the Major Colour Background eg. red For newtorking Majors.All text boxes are read only so the user can not change them-->
            <TextBox
                Background="White"
                IsReadOnly="true"
                Text="Prequsites" />
            <TextBox IsReadOnly="true" Text="T101 - Networking Funtalmentals " />
            <TextBox
                Background="White"
                IsReadOnly="true"
                Text="Major Electives" />
            <TextBox IsReadOnly="true" Text="T201 - Networking Funtalmentals" />
            <TextBox IsReadOnly="true" Text="T206 - CISCO RSE (Networks)" />
            <TextBox IsReadOnly="true" Text="T211 - Systems Security" />
        </StackPanel>
    </Flyout>
